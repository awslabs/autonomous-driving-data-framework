# This file should be invoked via source e.g. $ source source-activate-venv

echo "Activate Node Virtual Environment & Version"
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
nvm use
echo "Activate Python Virtual Environment ($PWD/.venv/bin/activate)"
source .venv/bin/activate
